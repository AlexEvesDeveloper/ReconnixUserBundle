{% extends "FOSUserBundle::layout.html.twig" %}

{% block fos_user_content %}
	<form action="{{ path('reconnix_user_new_index') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    	{{ form_widget(form) }}
    	{{ form_errors(form) }}
    	<div>
        	<input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    	</div>
	</form>
	<a href="{{path('reconnix_user_all_index'}}">Cancel</a>
{% endblock fos_user_content %}
